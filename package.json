		"@tailwindcss/forms": "^0.5.6",
		"@tailwindcss/typography": "^0.5.10",
		"@types/cookie": "^0.5.1",
		"autoprefixer": "^10.4.15",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"postcss": "^8.4.29",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "5.0.0-next.80",
		"svelte-check": "^3.4.3",
		"tailwindcss": "^3.3.3",
